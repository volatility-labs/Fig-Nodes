# Environment variables for fig-node
# Copy this file to .env and fill in your credentials.

# Polygon.io market data key
# Used by nodes: PolygonUniverseNode, PolygonCustomBarsNode, PolygonBatchCustomBarsNode,
# OrbFilterNode, IndustryFilterNode
POLYGON_API_KEY=

# Tavily web search key
# Used by node: WebSearchToolNode
TAVILY_API_KEY=

# Optional: Ollama API key if your setup requires it
# OLLAMA_API_KEY=
