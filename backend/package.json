{
  "name": "fig-node-backend",
  "version": "0.1.0",
  "description": "Fig-Node TypeScript Backend - Graph execution engine for agentic workflows",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/server/index.ts",
    "start": "node dist/server/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "fastify": "^5.2.1",
    "@fastify/websocket": "^11.0.1",
    "@fastify/cors": "^10.0.2",
    "@fastify/static": "^8.0.4",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "typescript": "^5.7.3",
    "tsx": "^4.19.2",
    "vitest": "^3.0.4",
    "eslint": "^9.18.0",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
